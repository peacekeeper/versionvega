<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  
  <RDF:Description RDF:about="urn:mozilla:install-manifest">
    <em:id>xpcomviewer@ondrejd.com</em:id>
    <em:name>XPCOMViewer</em:name>
    <em:description>XPCOMViewer - Explore world of XPCOM!</em:description>
    <em:version>0.9.1</em:version>
    <em:type>2</em:type>
    <em:creator>Ondrej Donek</em:creator>
    <em:homepageURL>http://xpcomviewer.mozdev.org</em:homepageURL>
    <em:optionsURL>chrome://xpcomviewer/content/preferences/preferences.xul</em:optionsURL>
    <em:aboutURL>chrome://xpcomviewer/content/about.xul</em:aboutURL>
    <em:iconURL>chrome://xpcomviewer/skin/xpcomviewer32.png</em:iconURL>
    
    <em:file>
      <!-- This is here for applications based on older versions of Gecko.
         - Do not remove it! -->
      <RDF:Description RDF:about="urn:mozilla:extension:file:xpcomviewer.jar">
        <em:package>chrome/content/</em:package>
        <em:locale>chrome/locale/cs-CZ/</em:locale>
        <em:locale>chrome/locale/en-US/</em:locale>
        <em:skin>chrome/skin/classic/</em:skin>
      </RDF:Description>
    </em:file>
    
    <em:targetApplication>
      <Description>
        <em:id>{crossinglight@versionvega}</em:id>
        <em:minVersion>0.1</em:minVersion>
        <em:maxVersion>1.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  
  </RDF:Description>
</RDF:RDF>
